{
  "name": "changelog-dashlet",
  "version": "0.0.1",
  "description": "Shows the changelog of the current vortex version on the dashlet",
  "main": "./out/index.js",
  "repository": "",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack --config webpack.config.js --display-error-details --progress --profile --color",
    "copy_scss": "copyfiles -f src/changelog.scss dist",
    "build": "npm run webpack && npm run copy_scss && extractInfo"
  },
  "author": "Black Tree Gaming Ltd.",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "@types/bluebird": "3.5.20",
    "@types/react": "^16.9.17",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.4",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/semver": "^5.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "copyfiles": "^2.1.1",
    "react-redux": "^5.0.7",
    "redux-act": "^1.7.4",
    "semver": "^5.5.0",
    "ts-loader": "^5.3.3",
    "typescript": "^2.9.2",
    "vortex-api": "Nexus-Mods/Vortex-api#electron7",
    "webpack": "^4.29.6"
  }
}
